{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read https://deno.land/std@0.157.0/http/file_server.ts dist/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "npm:react@^18.3",
    "jsxImportSourceTypes": "npm:@types/react@^18.3",
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ]
  },
  "imports": {
    "@types/axios": "npm:@types/axios@^0.14.0",
    "@types/react": "npm:@types/react@^18.3.11",
    "@types/react-bootstrap": "npm:@types/react-bootstrap@^1.0.1",
    "@types/react-dom": "npm:@types/react-dom@^18.3.0",
    "axios": "npm:axios@^1.7.7",
    "bootstrap": "npm:bootstrap@^5.3.3",
    "jotai": "npm:jotai@^2.10.0",
    "react": "npm:react@^18.3.1",
    "react-bootstrap": "npm:react-bootstrap@^2.10.5",
    "react-dom": "npm:react-dom@^18.3.1",
    "react-router-dom": "npm:react-router-dom@^6.26.2",
    "react-toastify": "npm:react-toastify@^10.0.5",
    "styled-components": "npm:styled-components@^6.1.13"
  }
}
